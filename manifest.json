{
    "name": "Annoying headers blocker",
    "version": "1.0.0",
    "description": "This is an extension that blocks annoying headers",
    "manifest_version": 3,
    "host_permissions": ["<all_urls>"],
    "permissions": ["scripting"],
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["script.js"],
          "run_at": "document_end"
        }
    ]
}